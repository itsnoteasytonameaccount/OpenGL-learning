#include "Camera.h"
#include <GLFW/glfw3.h>

int main(){
    glfwInit();
    Camera *camera = new Camera(800,600);
    std::cout << "now:" << glfwGetTime() << std::endl;
    for (int i = 0; i < 100000; i++)
    {
        camera->processMouseMovement(0, 0);
        camera->getViewMatrix();
    }
    std::cout << "end:" << glfwGetTime() << std::endl;
    return 0;
}