
main:myobject myopengl $(COMPILE_PATH)

ifdef SLIENT
	SLIENT_OPTION = -s
endif

myopengl:
	$(MAKE) $(SLIENT_OPTION) -f c/libs/makefile

myobject:
	$(MAKE) $(SLIENT_OPTION) -f c/object/makefile

# 编译方式可能有些许差别，所以每个单独写一个文件
$(COMPILE_PATH):
	$(MAKE) $(SLIENT_OPTION) -f c/$@/makefile CURRENT_PATH=$@